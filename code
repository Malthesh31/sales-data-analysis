bbyfonline <- filter(salesrecord,`Item Type`=="Baby Food" & `Sales Channel`==0)
nrow(bbyfonline)
View(bbyfonline)
bbyfoffline <- filter(salesrecord,`Item Type`=="Baby Food" & `Sales Channel`==1)
nrow(bbyfoffline)
View(bbyfoffline)
#Snacks
